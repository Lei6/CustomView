<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ui_view_MusicView">
        <attr name="ui_view_music_textColor" format="color"/>
        <attr name="ui_view_music_textColorCur" format="color"/>
        <attr name="ui_view_music_timeColor" format="color"/>
        <attr name="ui_view_music_textSize" format="dimension"/>
        <attr name="ui_view_music_textSizeCur" format="dimension"/>
        <attr name="ui_view_music_timeSize" format="dimension"/>
        <attr name="ui_view_music_padding" format="dimension"/>
        <attr name="ui_view_music_minScale" format="float"/>
        <attr name="ui_view_music_maxScale" format="float"/>
    </declare-styleable>
    
    <declare-styleable name="ui_view_ToggleButton">
        <attr name="ui_view_colorThumb" format="color"/>
        <attr name="ui_view_colorTrackOpen" format="color"/>
        <attr name="ui_view_colorTrackOff" format="color"/>
        <attr name="ui_view_colorPadding" format="color"/>
        <attr name="ui_view_padding" format="dimension"/>
        <attr name="ui_view_duration" format="integer"/>
    </declare-styleable>

    <declare-styleable name="ui_view_SideBar">
        <attr name="ui_view_sidebar_type">
            <enum name="normal" value="0"/>
            <enum name="center" value="1"/>
        </attr>
        <attr name="ui_view_sidebar_letters" format="string"/>
        <attr name="ui_view_sidebar_maxCount" format="integer"/>
    </declare-styleable>

</resources>